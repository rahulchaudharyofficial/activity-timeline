<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Object_Color_required_with_Icon_Image</fullName>
    <active>true</active>
    <description>Object Color required with Icon Image</description>
    <errorConditionFormula>AND(LEN(Icon_Image_Url__c)&gt;0, LEN(Object_Color__c) = 0)</errorConditionFormula>
    <errorDisplayField>Object_Color__c</errorDisplayField>
